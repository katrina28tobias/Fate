<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.menaces.fate">

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Fate"
        tools:targetApi="31">
        <activity
            android:name=".unexpectedEncounter.AskDateActivity"
            android:exported="false"
            android:parentActivityName=".StoryTrialActivity"/>
        <activity
            android:name=".StoryTrialActivity"
            android:exported="false"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".unexpectedEncounter.NameErrorActivity"
            android:exported="false"
            android:parentActivityName=".StoryTrialActivity" />
        <activity
            android:name=".unexpectedEncounter.CoffeeShopActivity"
            android:exported="false"
            android:parentActivityName=".StoryTrialActivity" />
        <activity
            android:name=".StoryMenuActivity"
            android:exported="false"
            android:parentActivityName=".MainActivity">
            <meta-data
                android:name="android.app.lib_name"
                android:value="" />
        </activity>
        <activity
            android:name=".GenreMenuActivity"
            android:exported="false" />
        <activity
            android:name=".MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>